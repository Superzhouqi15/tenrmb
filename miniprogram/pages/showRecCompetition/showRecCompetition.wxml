<!--pages/showRecCompetition/showRecCompetition.wxml-->
<view>
  <wux-cell-group title="赛事详情">
    <wux-cell thumb="/icon/competition.png" title="赛事名称" extra="{{dat[index].title}}"></wux-cell>
    <wux-cell thumb="/icon/organization.png" title="举办单位" extra="{{dat[index].organization}}"></wux-cell>
    <wux-cell thumb="/icon/publisher.png" title="发布者" extra="内容"></wux-cell>
  </wux-cell-group>

  <wux-cell-group title="赛事内容">
    <wux-cell extra="内容"></wux-cell>
  </wux-cell-group>

  <wux-cell-group title="参赛对象">
    <wux-cell extra="内容"></wux-cell>
  </wux-cell-group>

  <wux-cell-group title="报名方式">
    <wux-cell extra="内容"></wux-cell>
  </wux-cell-group>

  <wux-cell-group title="赛事相关时间">
    <wux-cell thumb="/icon/time.png" title="发布时间" extra="{{dat[index].title}}"></wux-cell>
    <wux-cell thumb="/icon/time.png" title="截止时间" extra="{{dat[index].organization}}"></wux-cell>
    <wux-cell thumb="/icon/label.png" title="时间标签" extra="内容"></wux-cell>
  </wux-cell-group>

  <wux-cell-group title="赛事配图">
    <wux-cell extra="内容"></wux-cell>
  </wux-cell-group>
  <view><br/></view>
</view>
