<!--miniprogram/pages/mark/mark.wxml-->
<scroll-view class='tab' scroll-y style='height:{{windowHeight-30}}px;'>
       <i-card wx:for='{{competition}}' title='{{item.title}}' thumb="{{item.thumb}}" bindtap="InToGame" wx:key="*this">
          <view slot="content">{{item.time}}</view>
          <view slot="footer">{{item.footer}}</view>
        </i-card>
      </scroll-view>
